// KIO Scopes: Nested blocks and variable visibility

let x = "global";
print "Start global x: " + x;

{
    let x = "inner";
    print "Inside block x: " + x;
    
    {
        let y = "deeper";
        print "Inside nested block y: " + y;
        print "Still see outer x: " + x;
    }
}

print "Back to global x: " + x;
