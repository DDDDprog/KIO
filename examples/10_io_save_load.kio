// KIO IO: Saving and Loading variable state
// This feature exports the environment to a JSON-like format.

let sessionID = 98765;
let user = "Dev";
let points = 50.5;

print "Saving state to session.json...";
save "session.json";

// Clear variables by reassigning
points = 0;
print "Points cleared to:";
print points;

print "Loading state back from session.json...";
load "session.json";

print "Restored points:";
print points;
print "Restored user:";
print user;
