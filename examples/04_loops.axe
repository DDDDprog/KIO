// Axeon Loops: While Statements

let count = 5;

print "Countdown starting:";

while (count > 0) {
    print count;
    count = count - 1;
}

print "Blast off!";

// Summing numbers 1 to 10
let sum = 0;
let i = 1;

while (i <= 10) {
    sum = sum + i;
    i = i + 1;
}

print "Sum of numbers 1 to 10:";
print sum;
