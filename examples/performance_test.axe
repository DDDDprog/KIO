// Axeon Performance Test
print "--- Axeon Performance Test ---";

let iterations = 1000000;
let sum = 0;
let i = 0;

let start = sys "time";

while (i < iterations) {
    sum = sum + i * 2 + 1;
    if (i % 1000 == 0) {
        sum = sum - 1;
    }
    i = i + 1;
}

let endTime = sys "time";

print "Iterations: " + iterations;
print "Final Sum: " + sum;
print "Time (ms): " + (endTime - start);
print "Performance test complete!";
