// Axeon Parallel Execution Test
print "--- Axeon Parallel Test ---";

let sum = 0;
let i = 0;

let start = sys "time";

while (i < 1000000) {
    sum = sum + i;
    i = i + 1;
}

let endTime = sys "time";

print "Sum: " + sum;
print "Time: " + (endTime - start) + "ms";
print "Parallel execution completed!";
