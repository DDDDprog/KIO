// KIO Arithmetic: Standard Math Operations

let a = 100;
let b = 15;

print "a = 100, b = 15";
print "Addition (a + b):";
print a + b;

print "Subtraction (a - b):";
print a - b;

print "Multiplication (a * b):";
print a * b;

print "Division (a / b):";
print a / b;

print "Modulo (Remainder) (a % b):";
print a % b;

print "Complex Expression (a + b * 2 / 10):";
print a + b * 2 / 10;
