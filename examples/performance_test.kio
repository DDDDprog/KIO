let start_time = sys("time");

// Test arithmetic performance
let sum = 0;
for i in range(1000000) {
    sum = sum + i * 2 - 1;
}

// Test string operations
let text = "";
for i in range(10000) {
    text = text + "fast";
}

// Test array operations
let arr = [];
for i in range(100000) {
    arr.push(i);
}

// Test parallel execution
parallel {
    let result1 = fibonacci(35);
    let result2 = factorial(20);
    let result3 = prime_count(100000);
}

let end_time = sys("time");
print("Performance test completed in: " + (end_time - start_time) + "ms");
print("Sum: " + sum);
print("Text length: " + text.length);
print("Array length: " + arr.length);
