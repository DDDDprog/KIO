{
  "Print": {
    "scope": "kio",
    "prefix": "kio-print",
    "body": [
      "print \"$1\";"
    ],
    "description": "Print a value"
  },
  "Let": {
    "scope": "kio",
    "prefix": "kio-let",
    "body": [
      "let ${1:name} = ${2:value};"
    ],
    "description": "Variable declaration"
  },
  "IfElse": {
    "scope": "kio",
    "prefix": "kio-if",
    "body": [
      "if (${1:condition}) {",
      "    $0",
      "} else {",
      "    ",
      "}"
    ],
    "description": "If/Else block"
  },
  "While": {
    "scope": "kio",
    "prefix": "kio-while",
    "body": [
      "while (${1:condition}) {",
      "    $0",
      "}"
    ],
    "description": "While loop"
  }
}
